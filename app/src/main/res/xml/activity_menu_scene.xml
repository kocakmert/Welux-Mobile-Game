    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@+id/start"
        app:autoTransition="animateToEnd"
        app:duration="10000">
        <KeyFrameSet >
            <KeyPosition
                app:motionTarget="@+id/imageView2"
                app:framePosition="50"
                app:keyPositionType="parentRelative"
                app:percentX="0.3" />
            <KeyPosition
                app:motionTarget="@+id/imageView4"
                app:keyPositionType="parentRelative"
                app:percentX="0.5" />
            <KeyPosition
                app:motionTarget="@+id/imageView9"
                app:framePosition="50"
                app:keyPositionType="parentRelative"
                app:percentX="0.3" />
            <KeyPosition
                app:motionTarget="@+id/imageView11"
                app:framePosition="50"
                app:keyPositionType="parentRelative"
                app:percentX="0.2" />
        </KeyFrameSet>
    </Transition>
    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@+id/start"
        app:autoTransition="jumpToStart"
        />
</MotionScene>
