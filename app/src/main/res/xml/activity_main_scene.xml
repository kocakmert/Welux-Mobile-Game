<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@+id/level"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/adView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="5dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/sum"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_marginBottom="50dp"
            app:layout_constraintStart_toEndOf="@+id/absulute"
            app:layout_constraintEnd_toStartOf="@+id/block" />
        <Constraint
            app:layout_editor_absoluteY="564dp"
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/absulute"
            app:layout_constraintEnd_toStartOf="@+id/sum"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_marginBottom="50dp" />
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/block"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_marginBottom="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintStart_toEndOf="@+id/sum"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/right_button"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="70dp"
            android:layout_marginEnd="70dp"
            app:layout_constraintStart_toEndOf="@+id/left_button"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="220dp"
            app:layout_constraintBottom_toTopOf="@+id/sum"
            android:layout_marginBottom="80dp" />
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/left_button"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="70dp"
            android:layout_marginStart="70dp"
            app:layout_constraintEnd_toStartOf="@+id/right_button"
            app:layout_constraintBottom_toTopOf="@+id/sum"
            android:layout_marginBottom="80dp"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="220dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/x_left"
            app:layout_constraintTop_toBottomOf="@+id/left_button"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView10"
            android:layout_marginRight="18dp"
            android:layout_marginEnd="18dp" />
        <Constraint
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="109dp"
            android:layout_width="wrap_content"
            android:id="@+id/left_adet"
            app:layout_constraintTop_toBottomOf="@+id/left_button"
            android:layout_marginTop="4dp"
            app:layout_constraintStart_toEndOf="@+id/x_left"
            android:layout_marginLeft="3dp"
            android:layout_marginStart="3dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/x"
            app:layout_constraintTop_toBottomOf="@+id/right_button"
            android:layout_marginTop="4dp"
            app:layout_constraintStart_toEndOf="@+id/right_button" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/right_adet"
            app:layout_constraintStart_toEndOf="@+id/x"
            android:layout_marginLeft="3dp"
            android:layout_marginStart="3dp"
            app:layout_constraintTop_toBottomOf="@+id/right_button"
            android:layout_marginTop="4dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/rules"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/right_button"
            android:layout_marginBottom="50dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/game_level_count"
            app:layout_constraintStart_toEndOf="@+id/level"
            android:layout_marginLeft="2dp"
            android:layout_marginStart="2dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="12dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/coin"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="13dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="21dp"
            android:layout_marginEnd="21dp" />
        <Constraint
            android:layout_height="25dp"
            android:layout_width="25dp"
            android:id="@+id/diamomd"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="12dp"
            app:layout_editor_absoluteX="356dp"
            app:layout_constraintEnd_toStartOf="@+id/coin"
            android:layout_marginRight="5dp"
            android:layout_marginEnd="5dp" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/retry"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/btn_home"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="70dp"
            android:layout_marginStart="70dp"
            app:layout_constraintEnd_toStartOf="@+id/right_button"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="220dp"
            app:layout_constraintBottom_toTopOf="@+id/sum"
            android:layout_marginBottom="80dp" />
        <Constraint
            android:id="@+id/imageView8"
            android:layout_width="70dp"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="70dp"
            android:layout_marginEnd="70dp"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="200dp"
            app:layout_constraintStart_toEndOf="@+id/imageView10" />
        <Constraint
            app:layout_editor_absoluteY="492dp"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="329dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView20" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintTop_toBottomOf="@+id/diamomd" />
        <Constraint
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_editor_absoluteX="8dp"
            android:id="@+id/btn_home"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:id="@+id/simpleProgressBar"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="40dp"
            android:scaleY="6"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintStart_toEndOf="@+id/imageView24"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toBottomOf="@+id/level"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            android:id="@+id/imageView24"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:id="@+id/timer"
            app:layout_constraintTop_toBottomOf="@+id/simpleProgressBar"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="0dp"
            android:layout_marginStart="0dp" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">
        <Constraint android:id="@id/level" />
        <Constraint
            android:layout_height="90dp"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_width="70dp"
            android:id="@+id/imageView10"
            android:layout_marginBottom="70dp"
            app:layout_constraintStart_toEndOf="@+id/absulute"
            app:layout_constraintEnd_toStartOf="@+id/block" />
        <Constraint
            android:id="@+id/imageView8"
            android:layout_width="70dp"
            android:layout_height="100dp"
            app:layout_constraintStart_toEndOf="@+id/absulute"
            app:layout_constraintEnd_toStartOf="@+id/block"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_marginBottom="70dp" />

    </ConstraintSet>
    <ConstraintSet android:id="@+id/step3">
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/imageView12"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="220dp"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="70dp"
            android:layout_marginEnd="70dp"
            app:layout_constraintBottom_toTopOf="@+id/sum"
            android:layout_marginBottom="80dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step4">
        <Constraint
            android:layout_height="90dp"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_width="70dp"
            android:id="@+id/imageView12"
            android:layout_marginBottom="70dp"
            app:layout_constraintStart_toEndOf="@+id/absulute"
            app:layout_constraintEnd_toStartOf="@+id/block" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/coin_start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/diamomd"
            android:id="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/coin_finish">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/coin"
            android:layout_marginTop="20dp"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/block_start">
        <Constraint
            android:id="@+id/left_block_motion"
            android:layout_width="70dp"
            android:layout_height="90dp"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="220dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="70dp"
            android:layout_marginStart="70dp"
            app:layout_constraintEnd_toStartOf="@+id/right_button"
            app:layout_constraintBottom_toTopOf="@+id/sum"
            android:layout_marginBottom="80dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/block_finish">
        <Constraint
            android:layout_width="70dp"
            android:layout_height="90dp"
            android:id="@+id/left_block_motion"
            app:layout_constraintStart_toEndOf="@+id/sum"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_marginBottom="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/block_start_right">
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            android:id="@+id/right_block_animation"
            app:layout_constraintTop_toBottomOf="@+id/level"
            android:layout_marginTop="220dp"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="70dp"
            android:layout_marginEnd="70dp"
            app:layout_constraintBottom_toTopOf="@+id/sum"
            android:layout_marginBottom="80dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/block_finish_right">
        <Constraint
            android:layout_height="90dp"
            android:layout_width="70dp"
            app:layout_constraintStart_toEndOf="@+id/sum"
            android:id="@+id/right_block_animation"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toTopOf="@+id/adView"
            android:layout_marginBottom="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="25dp"
            android:layout_marginEnd="25dp" />
        <Constraint
            app:layout_editor_absoluteY="243dp"
            android:layout_height="90dp"
            app:layout_editor_absoluteX="249dp"
            android:layout_width="70dp"
            android:id="@+id/imageView10" />
    </ConstraintSet>
    <Transition
        app:constraintSetEnd="@id/end"
        app:constraintSetStart="@+id/start"
        app:autoTransition="animateToStart"
        app:duration="900"/>
    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@+id/start"
        app:autoTransition="jumpToStart"
        app:duration="900"
        />
    <Transition
        app:constraintSetEnd="@id/step4"
        app:constraintSetStart="@+id/step3"
        app:autoTransition="animateToStart"
        app:duration="900"/>
    <Transition
        app:constraintSetEnd="@+id/step4"
        app:constraintSetStart="@+id/step3"
        app:autoTransition="jumpToStart"
        app:duration="900"
        />
    <Transition
        app:constraintSetEnd="@id/coin_finish"
        app:constraintSetStart="@+id/coin_start"
        app:autoTransition="animateToStart"
        app:duration="1300"/>
    <Transition
        app:constraintSetEnd="@+id/coin_finish"
        app:constraintSetStart="@+id/coin_start"
        app:autoTransition="jumpToStart"
        app:duration="1300"
        />
    <Transition
        app:constraintSetEnd="@id/block_finish"
        app:constraintSetStart="@+id/block_start"
        app:autoTransition="animateToStart"
        app:duration="900"/>
    <Transition
        app:constraintSetEnd="@+id/block_finish"
        app:constraintSetStart="@+id/block_start"
        app:autoTransition="jumpToStart"
        app:duration="900"
        />
    <Transition
        app:constraintSetEnd="@id/block_finish_right"
        app:constraintSetStart="@+id/block_start_right"
        app:autoTransition="animateToStart"
        app:duration="900"/>
    <Transition
        app:constraintSetEnd="@+id/block_finish_right"
        app:constraintSetStart="@+id/block_start_right"
        app:autoTransition="jumpToStart"
        app:duration="900"
        />

</MotionScene>
